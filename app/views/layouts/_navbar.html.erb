<header class="header" >
  <%= link_to image_tag('logo.svg') , root_path %>
  <div class="header__actions">
    <% if user_signed_in? %>
      <%= link_to 'Edit Profile', edit_user_registration_path, class: "header__link" %>
      <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: "header__link" %>
    <% else %>
      <%= link_to 'Login', new_user_session_path %>
      <%= link_to "Sign up", new_user_registration_path, class: 'header__link'  %>
    <% end %>
  </div>
</header>